{
    "name": "@abdt/design-tokens",
    "version": "0.0.10",
    "description": "ABDT's Design tokens",
    "license": "MIT",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "typings": "dist/esm/index.d.ts",
    "engines": {
        "node": ">=10"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepare": "yarn build",
        "start": "tsc --watch",
        "prebuild": "rimraf dist",
        "build": "tsc && tsc --module commonjs --target es5 --outDir dist/cjs",
        "test": "jest",
        "clean": "rimraf dist && rimraf node_modules",
        "eslint": "eslint --ext .ts,.tsx src --fix"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/react": "^17.0.0"
    },
    "sideEffects": false
}
